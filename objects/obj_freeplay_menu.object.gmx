            <string>///Navigating
// Move down
if obj_input.down {
    menu_index += 1;

    // Wrap to first
    if (menu_index &gt;= menu_length) {
        menu_index = 0;
        menu_window_start = 0; // reset window to top
    }

    // Scroll window if needed
    if (menu_index &gt;= menu_window_start + menu_window_size) {
        menu_window_start = menu_index - menu_window_size + 1;
    }
}

// Move up
if obj_input.up {
    menu_index -= 1;

    // Wrap to last
    if (menu_index &lt; 0) {
        menu_index = menu_length - 1;
        menu_window_start = max(0, menu_length - menu_window_size); 
    }

    // Scroll window if needed
    if (menu_index &lt; menu_window_start) {
        menu_window_start = menu_index;
    }
}
