        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// --- Load song &amp; chart ---

// receptors
var px = global.player_offset_x;
for (var i = 0; i &lt; 4; i++)
{
    instance_create(px + i*80, global.offset_y-5, obj_receptor);
}

var bx = global.opponent_offset_x;
for (var i = 0; i &lt; 4; i++)
{
    instance_create(bx + i*80, global.offset_y-5, obj_receptor);
}

// note scroll direction: up (classic FNF)
note_spawn_y = global.offset_y+600;
hit_y = global.offset_y;
global.countdown_state = 0;
song_pos = 0;
score = 0;
global.life = 50;

sfx_path = working_directory + "sounds/";
snd_count3 = audio_create_stream(sfx_path + "intro3.ogg");
snd_count2 = audio_create_stream(sfx_path + "intro2.ogg");
snd_count1 = audio_create_stream(sfx_path + "intro1.ogg");
snd_go = audio_create_stream(sfx_path + "introGo.ogg");
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Notes

if (!global.song_started)
{
    global.song_delay -= delta_time/1000000; // or: 1/room_speed

    if (global.song_delay &lt;= 0)
    {
        global.stream = audio_play_sound(global.inst_stream, 0, false);
        if global.voices_stream != -1 audio_play_sound(global.voices_stream, 1, false);
        global.song_started = true;
    }
    else
    {
        // IMPORTANT: make fake song_pos negative
        song_pos = -global.song_delay;
    }
}
else
{
    song_pos = audio_sound_get_track_position(global.stream);
}

// distance from spawn Y to receptor
var distance = (room_height + 50) - global.offset_y; 

// speed = pixels per step, step = 1/room_speed seconds
// convert speed to pixels per second:
var speed_per_sec = global.scroll_speed * room_speed; 

// spawn_offset in seconds = distance / speed_per_sec
var spawn_offset = distance / speed_per_sec;

for (var i = 0; i &lt; ds_list_size(global.notes); i++)
{
    var note = global.notes[| i];
    var t    = note[? "time"];
    var dir  = note[? "dir"];

    if (!note[? "spawned"] &amp;&amp; song_pos &gt;= t - spawn_offset)
    {
        var offset_base;

        if (note[? "isPlayer"])
            offset_base = global.player_offset_x;
        else
            offset_base = global.opponent_offset_x;
        
        var nx = offset_base + dir * 80;  // 80 = lane spacing
        var ny = room_height + 50; // spawn 50 pixels below the bottom of the room
        
        var inst = instance_create(nx, ny, obj_note);
        inst.note_time = t;
        inst.dir       = dir;
        inst.noteType  = note[? "noteType"];
        inst.speed     = global.scroll_speed;
        inst.isPlayer  = note[? "isPlayer"];

        note[? "spawned"] = true;
        global.notes[| i] = note;
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="8" enumb="64">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
