        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>draw_text(2,2,"



================
SONG NAME: " + global.song_name + "
DIFFICULTY: " + string_upper(global.difficulty) + "

BPM: " + string(global.bpm) + "
SPEED: " + string(global.scroll_speed) + "
OPPONENT: " + global.stage_player2 + "
PLAYER: " + global.stage_player1 + "

SCORE: " + string(score) +"
================
");

// Draw Event
draw_set_halign(fa_center);
draw_set_valign(fa_middle);
draw_set_color(c_white);

draw_healthbar(390,680,890,690,global.life,c_red,c_lime,c_lime,1,true,true);

if (!global.song_started)
{
    var beat = global.sec_per_beat;

    // ----- "3" -----
    if (song_pos &gt;= -(4 * beat) &amp;&amp; song_pos &lt; -(3 * beat))
    {
        draw_text(640, 360, "3");

        if (global.countdown_state &lt; 1)
        {
            audio_play_sound(snd_count3, 1, false);
            global.countdown_state = 1;
        }
    }
    // ----- "2" -----
    else if (song_pos &gt;= -(3 * beat) &amp;&amp; song_pos &lt; -(2 * beat))
    {
        draw_text(640, 360, "2");

        if (global.countdown_state &lt; 2)
        {
            audio_play_sound(snd_count2, 1, false);
            global.countdown_state = 2;
        }
    }
    // ----- "1" -----
    else if (song_pos &gt;= -(2 * beat) &amp;&amp; song_pos &lt; -(beat))
    {
        draw_text(640, 360, "1");

        if (global.countdown_state &lt; 3)
        {
            audio_play_sound(snd_count1, 1, false);
            global.countdown_state = 3;
        }
    }
    // ----- "GO!" -----
    else if (song_pos &gt;= -(beat) &amp;&amp; song_pos &lt; 0)
    {
        draw_text(640, 360, "GO!");

        if (global.countdown_state &lt; 4)
        {
            audio_play_sound(snd_go, 1, false);
            global.countdown_state = 4;
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>
